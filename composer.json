{
    "name": "equinesolutiuons/ioc-filemaker",
    "description": "A communication wrapper between laravel and ioc filemaker db",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "EquineSolutions\\IOCFilemaker\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EquineSolutions\\IOCFilemaker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EquineSolutions\\IOCFilemaker\\Providers\\IOCFilemakerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
