{
    "name": "equinesolutiuons/ioc-filemaker",
    "description": "A communication wrapper between laravel and ioc filemaker db",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "airmoi/filemaker": "*",
        "laravel/framework": "6.0",
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "^8.3"
    },
    "require-dev": {
        "orchestra/testbench": "4.0.1"
    },
    "autoload": {
        "psr-4": {
            "EquineSolutions\\IOCFilemaker\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EquineSolutions\\IOCFilemaker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "EquineSolutions\\IOCFilemaker\\Providers\\IOCFilemakerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
